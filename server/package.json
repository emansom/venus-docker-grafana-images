		"mqtt": "^4.2.8",
		"node-esm-loader": "^0.0.1-alpha",
		"node-fetch": "^3.0.0",
		"node-ssdp": "^4.0.1",
		"primus": "^8.0.5",
		"react-markdown": "^7.0.1",
		"tsscmp": "^1.0.6",
		"winston": "^3.3.3",
		"ws": "^8.2.3",
		"xml2js": "^0.4.23"
	},
